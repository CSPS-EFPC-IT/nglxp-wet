#}

{% if entry is defined %}

    {# Title #}
    <meta name="dcterms.title" content="{{ entry.title }}">

    {# Abstract #}
    {% if entry.dcAbstract is defined and entry.dcAbstract is not empty %}
        <meta name="dcterms.abstract" content="{{ entry.dcAbstract }}">
    {% endif %}

