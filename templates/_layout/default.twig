{% set shortlang = currentSite.language|slice(0,2) %}

<!DOCTYPE html><!--[if lt IE 9]><html class="no-js lt-ie9" lang="{{ shortlang }}" dir="ltr"><![endif]--><!--[if gt IE 8]><!-->
<html class="no-js" lang="{{ shortlang }}" dir="ltr">
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta content="width=device-width,initial-scale=1" name="viewport">
    {# TODO: Add dynamic site title #}
    <title>{{ entry.title }} - {{ siteName }}</title>
    <meta name="description" content="">
    <!--[if gte IE 9 | !IE ]><!-->
        <link href="/dist/gcweb/assets/favicon.ico" rel="icon" type="image/x-icon">
        <link rel="stylesheet" href="/dist/gcweb/css/theme.min.css">
        <link rel="stylesheet" href="{{ mix('css/app.css') }}">
    <!--<![endif]-->
    <!--[if lt IE 9]>
        <link href="/dist/gcweb/assets/favicon.ico" rel="shortcut icon" />
        <link rel="stylesheet" href="/dist/gcweb/css/ie8-theme.min.css" />
        <script src="http://ajax.googleapis.com/ajax/libs/jquery/1.12.4/jquery.min.js"></script>
        <script src="/dist/wet-boew/js/ie8-wet-boew.min.js"></script>

    <![endif]-->
    <noscript><link rel="stylesheet" href="/dist/wet-boew/css/noscript.min.css" /></noscript>
    {% block customStyles %}{% endblock %}
    {{ head() }}
</head>
<body{% if entry %} class="entry--{{ entry.section.handle }}"{% endif %} vocab="http://schema.org/" typeof="WebPage">
    {{ beginBody() }}
    {% block page %}{% endblock %}
    <!--[if gte IE 9 | !IE ]><!-->
        <script src="https://ajax.googleapis.com/ajax/libs/jquery/2.2.4/jquery.js"></script>
        <script src="/dist/wet-boew/js/wet-boew.min.js"></script>
    <!--<![endif]-->
    <!--[if lt IE 9]>
        <script src="/dist/wet-boew/js/ie8-wet-boew2.min.js"></script>
    <![endif]-->
    <script src="/dist/gcweb/js/theme.min.js"></script>
    <script src="{{ mix('js/app.js') }}"></script>
    {% block customScripts %}{% endblock %}
    {{ endBody() }}
</body>
</html>