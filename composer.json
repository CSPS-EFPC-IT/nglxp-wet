{
  "require": {
    "aelvan/imager": "v2.1.10",
    "anubarak/craft-relabel": "1.3.2.2",
    "craftcms/cms": "3.2.2",
    "craftcms/feed-me": "4.1.1",
    "craftcms/redactor": "2.3.3.2",
    "ether/tags": "1.0.5",
    "fruitstudios/linkit": "1.1.11",
    "mildlygeeky/craft-kint": "1.0.0",
    "misterbk/mix": "1.5.2",
    "nystudio107/craft-emptycoalesce": "1.0.6",
    "nystudio107/craft-templatecomments": "1.1.0",
    "nystudio107/craft-units": "1.0.3",
    "rias/craft-width-fieldtype": "1.0.6",
    "spicyweb/craft-neo": "2.3.5",
    "topshelfcraft/wordsmith": "3.0.5",
    "verbb/field-manager": "2.1.0",
    "verbb/image-resizer": "2.0.5",
    "verbb/navigation": "1.1.12",
    "verbb/super-table": "2.2.1",
    "vlucas/phpdotenv": "^2.4.0"
  },
  "autoload": {
    "psr-4": {
      "modules\\": "modules/"
    }
  },
  "config": {
    "sort-packages": true,
    "optimize-autoloader": true,
    "platform": {
      "php": "7.0"
    }
  },
  "scripts": {
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ]
  }
}
